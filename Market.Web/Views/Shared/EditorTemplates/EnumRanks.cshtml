@model string
@{
    var ranks = (IEnumerable<SelectListItem>)ViewData["ranks"];
    foreach (var rank in ranks)
    {
        if (rank.Value == Model)
        {
            rank.Selected = true;
        }
    }
}
@Html.DropDownList("Rank", ranks, new { @class = "form-control", id = "rank"})
